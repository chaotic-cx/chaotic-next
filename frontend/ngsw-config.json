{
  "$schema": "../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.csr.html", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"]
      }
    },
    {
      "name": "external-cdn",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://cdn.jsdelivr.net/gh/highlightjs/cdn-release/build/highlight.min.js",
          "https://rsms.me/inter/inter.css"
        ]
      }
    },
    {
      "name": "chaotic-router",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": ["https://status.chaotic.cx/mirrors.json"]
      }
    }
  ]
}
